-- initialization

function _init()


--  scene = 0 -- (x ??) startscreen
		scene = 1 -- == game, waiting
-- 2 == (o) - place_resource
-- 3 == (x) - end_screen / restart
  
  button_pressed = false 
  
  counter = 1
  
  timer = 1
  timermax = 9

	 cx = 64
	 cy = 120
	 
	 newx = 64
	 newy = 16
	 
	 draw_no = 1

	
	 step = 8   -- rasterweite
	 
	 
	 sel = 1
	 
	 
	 -- gameplay
	 resources()
	 draws()
	
end

