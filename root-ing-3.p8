function resources()

 res = {
 	{name="  return - no placing",col=7}, 
 	{name="much water",col=12},
 	{name="little water",col=12},
 	{name="copper",col=4},
 	{name="phosphate",col=14},
 	{name="sulphur",col=10},
 	{name="ferrum",col=13},
 	{name="stone",col=5},
 	{name="      restart the game",col=8}
 }
 
 res_count = #res

end	 



