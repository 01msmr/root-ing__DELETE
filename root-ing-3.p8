-- scene1: main processing


-- input monitoring and 
-- function calling

function _update()

	if scene == 1 then
		if 					btnp(⬅️) then
	 -- left
		 newx = cx - step
	 	
	 elseif  btnp(➡️) then
	 -- right
	  newx = cx + step
	 	
	 elseif  btnp(⬆️) then
	 -- up
	  newy = cy - step
	
	 elseif  btnp(⬇️) then
	 -- down
	  newy = cy + step
	  
	 elseif btnp(❎) then
	  scene = 2
	  
	  
   --sel = 1
	  selected = false
 	end
 
	elseif scene == 2 then
 	if btnp(⬅️) then
 	 sel -= 1
 	 button_press = true
	 	if sel < 1 then 
	 		sel = res_count-1
	 	end	
	 elseif btnp(➡️) then
 	 sel += 1
 	 button_press = true
	 	if sel > res_count then 
	 		sel = 1
	 	end	
	 elseif btnp(❎) then 
	 	scene = 1	
 	 button_press = true 
 	 write_resource() 
 	end
 end  
	 
 

	if timer == 1 then
	 if newx < 0 then newx = 0 end
 	if newx > 120 then newx = 120 end
 	if newy < 0 then newy = 0 end
 	if newy > 120 then newy = 120 end 
 end

 cx = newx
 cy = newy
 
 if timer > timermax 
 	then timer = 1
 	else timer += 1
 end
 
end



function _draw()
	 if timer == 1 then
  	cls(0) 
  	map(0, 0, 0, 0, 16, 16)
	 	spr_cursor()
	end
	
	
 
 if scene == 2 then
	 select_resource() 
	end
	
	
	--debug()
	--debug2()
	
end


