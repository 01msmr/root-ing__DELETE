-- scene2: resource/restart

function select_resource() 
 
 
	
 -- gray rect w. instructions
 rectfill(20,26,108,68,5)
 rect(21,26+1,107,67,13)
 rect(20,26,108,68,7)
 
 -- blue rect with selection
 rectfill(5,50,120,86,1)
 rect(6,51,120,86,13)
 rect(5,50,121,87,7)


 if button_pressed == true then 
 	b_col = 2
 else
 	b_col = 7
 end
 
 print("   ➡️",28,32,b_col)
 print("⬅️   ",28,32,b_col)
 print("❎",   28,41,b_col)
 
 
 
		--print selectionable elements
		
		--print element name & no
--		print("select element no.", 14,60, res[sel].col)
		print("select element no.", 14,60, 7)
		print(sel-1, 88,60, 7)
		
		--print sprite - in color - in row
		if sel == res_count then
			--print exit game (arrow)
			spr(71+sel,39,75)
		else
			--print selected resource
			spr(71+sel,14,72)
		end		
		--print elem. name (elem. color)
		if sel == res_count then 
			print(res[sel].name,29,77, res[sel].col)
		else
			print(res[sel].name,27,74, res[sel].col)
		end
	 
	 
	  
--	 for i = 2,sel do
--	  print("➡️", -2 + step*i,60,11)
--	 end
	 
	
end



function write_resource()
 if sel > 0 and sel < res_count 
 then
  draw[sel] = 
  {
  	[1]={dx=1, dy=1, draw_no=1}
  }
  
		draw[draw_no].dx = cx
		draw[draw_no].dy = cy
		draw[draw_no].ele = sel
		
 	draw_no += 1
	end
end
